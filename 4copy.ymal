  - name: 🇺🇸 美国
    type: url-test
    url: 'http://www.gstatic.com/generate_204'
    interval: 300
    tolerance: 50
    use:
      - 服务提供商
    filter: "(?i)美|US|CA|United States|America"

  - name: 🇯🇵 日本
    type: url-test
    url: 'http://www.gstatic.com/generate_204'
    interval: 300
    tolerance: 50
