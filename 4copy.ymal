port: 7890
socks-port: 7891
redir-port: 7892
allow-lan: true
mode: rule
log-level: warning
external-controller: 127.0.0.1:9090

dns:
  enable: true
  ipv6: false
  listen: 0.0.0.0:53
  enhanced-mode: fake-ip
  nameserver:
    - 114.114.115.115
    - 119.28.28.28
    - 223.6.6.6
    - https://dns.alidns.com/dns-query
    - https://doh.pub/dns-query
  fallback:
    - tcp://1.1.1.1
    - tcp://1.0.0.1

proxy-providers:
  服务提供商:
    type: http
    url: https://raw.githubusercontent.com/zhangkaiitugithub/passcro/main/speednodes.yaml
    interval: 3600
    path: ./proxies/服务提供商.yaml
    health-check:
      enable: true
      interval: 600
      url: http://www.gstatic.com/generate_204

proxy-groups:
  - name: 🚀 选择代理
    type: select
    proxies:
      - ♻️ 自动选择
      - 🇺🇸 美国
      - 🇯🇵 日本
      - 🇰🇷 韩国
      - 🇭🇰 香港
      - 🇸🇬 新加坡
      - 🇷🇺 俄罗斯
      - 🇪🇺 欧洲
      - DIRECT

  - name: ♻️ 自动选择
    type: url-test
    url: 'http://www.gstatic.com/generate_204'
    interval: 300
    tolerance: 50
    use:
      - 服务提供商

  - name: 🤖 GPT
    type: select
    proxies:
      - 🇺🇸 美国
      - 🇯🇵 日本
      - 🇰🇷 韩国
      - 🇸🇬 新加坡
      - 🇷🇺 俄罗斯
      - 🇪🇺 欧洲
      - ♻️ 自动选择

  - name: 🤖 Claude
    type: select
    proxies:
      - 🇺🇸 美国
      - 🇯🇵 日本
      - 🇰🇷 韩国
      - 🇸🇬 新加坡
      - 🇷🇺 俄罗斯
      - 🇪🇺 欧洲
      - ♻️ 自动选择

  - name: 🎭 Tiktok
